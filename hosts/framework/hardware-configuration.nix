    { device = "/dev/disk/by-uuid/5b46b57e-fcf3-4b7e-81e6-b247fa7dbd09";
      fsType = "ext4";
    };

  boot.initrd.luks.devices."luks-539f409b-ee7a-4707-a1ae-94c5d54828ab".device = "/dev/disk/by-uuid/539f409b-ee7a-4707-a1ae-94c5d54828ab";

